);\n    onMount(() => {\n      document.addEventListener(\"scroll\", handleScroll);\n    });\n    function handleScroll(e) {\n      height.value = e.target.documentElement.scrollTop;\n      if (e.target.documentElement.scrollTop > 50) scrollOver.value = true;\n      else if (e.target.documentElement.scrollTop < 80)\n        scrollOver.value = false;\n    }\n    return { scrollOver };\n  },\n};\n</script>\n\n<style lang=\"scss\">\nheader {\n  text-align: center;\n  background-image: url(\"@/assets/img/background.svg\");\n  background-position: center;\n  background-size: cover;\n  -webkit-transition: all 2s linear;\n  -moz-transition: all 2s linear;\n  -o-transition: all 2s linear;\n  transition: all 2s linear;\n}\n.sticky {\n  padding-top: 15px !important;\n  padding-bottom: 0 !important;\n  position: fixed;\n  width: 100%;\n  height: 50px;\n  text-align: left !important;\n  display: inline-flex;\n  * {\n    font-size: 1.2em;\n  }\n  span {\n    font-size: 1.2em;\n  }\n  img {\n    margin-top: -10px;\n    margin-right: 20px;\n    padding: 0;\n    width: 50px;\n    height: 50px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}