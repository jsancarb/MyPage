{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    let lastY = null;\n    let timer = true;\n    let page = 0;\n    let rep = 0;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleWhell, {\n        passive: false\n      });\n      document.addEventListener(\"touchmove\", handleTouch, {\n        passive: false\n      });\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleWhell(event) {\n      event.preventDefault();\n\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n\n      lastY = nowY;\n    }\n\n    function handlePage(num) {\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n\n        case 2:\n          visibleSkills.value = true;\n          break;\n      }\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuFA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CAAC,KAAD,CAA9B;IACA,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAD,CAAzB;IACA,IAAIU,KAAI,GAAI,IAAZ;IACA,IAAIC,KAAI,GAAI,IAAZ;IACA,IAAIC,IAAG,GAAI,CAAX;IACA,IAAIC,GAAE,GAAI,CAAV;IACAZ,SAAS,CAAC,MAAM;MACda,QAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmCC,WAAnC,EAAgD;QAAEC,OAAO,EAAE;MAAX,CAAhD;MACAH,QAAQ,CAACC,gBAAT,CAA0B,WAA1B,EAAuCG,WAAvC,EAAoD;QAAED,OAAO,EAAE;MAAX,CAApD;MACAE,WAAW,CAAC,MAAM;QAChBZ,MAAM,CAACa,KAAP,GAAgBb,MAAM,CAACa,KAAP,GAAe,EAA/B,GAAsCb,MAAM,CAACa,KAAP,GAAe,GAArD;MACD,CAFU,EAER,GAFQ,CAAX;IAGD,CANQ,CAAT;;IAOA,SAASJ,WAAT,CAAqBK,KAArB,EAA4B;MAC1BA,KAAK,CAACC,cAAN;;MACA,IAAIX,KAAJ,EAAW;QACTA,KAAI,GAAI,KAAR;QACAY,UAAU,CAAC,MAAM;UACfZ,KAAI,GAAI,IAAR;QACD,CAFS,EAEP,GAFO,CAAV;;QAGA,IAAIU,KAAK,CAACG,WAAN,GAAoB,CAApB,IAAyBZ,IAAG,GAAI,CAApC,EAAuC;UACrCA,IAAI;UACJa,UAAU,CAACb,IAAD,CAAV;QACF,CAHA,MAGO,IAAIS,KAAK,CAACG,WAAN,GAAoB,CAApB,IAAyBZ,IAAG,GAAI,CAApC,EAAuC;UAC5CA,IAAI;UACJa,UAAU,CAACb,IAAD,CAAV;QACF;MACF;IACF;;IACA,SAASM,WAAT,CAAqBG,KAArB,EAA4B;MAC1BA,KAAK,CAACC,cAAN;MACAT,GAAG;MACH,IAAIa,IAAG,GAAIL,KAAK,EAAEM,OAAP,CAAe,CAAf,EAAkBC,OAA7B;;MACA,IAAIF,IAAG,GAAIhB,KAAP,IAAgBE,IAAG,GAAI,CAAvB,IAA4BC,GAAE,GAAI,EAAtC,EAA0C;QACxCD,IAAI;QACJa,UAAU,CAACb,IAAD,CAAV;QACAC,GAAE,GAAI,CAAN;MACF,CAJA,MAIO,IAAIa,IAAG,GAAIhB,KAAP,IAAgBE,IAAG,GA