{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    let currentPos = 0;\n    onMounted(() => {\n      document.addEventListener(\"scroll\", handleScroll, false);\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleScroll(e) {\n      if (currentPos < e.target.documentElement.scrollTop) {\n        console.log(`mas pequeño: ${window.innerHeight}`);\n        currentPos += window.innerHeight;\n      } else {\n        console.log(`mas grande: ${window.innerHeight}`);\n        currentPos -= window.innerHeight;\n      }\n\n      window.scrollTo(0, currentPos);\n      if (currentPos > 0) handleWrite();\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6EA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,IAAIQ,UAAS,GAAI,CAAjB;IACAP,SAAS,CAAC,MAAM;MACdQ,QAAQ,CAACC,gBAAT,CAA0B,QAA1B,EAAoCC,YAApC,EAAkD,KAAlD;MACAC,WAAW,CAAC,MAAM;QAChBL,MAAM,CAACM,KAAP,GAAgBN,MAAM,CAACM,KAAP,GAAe,EAA/B,GAAsCN,MAAM,CAACM,KAAP,GAAe,GAArD;MACD,CAFU,EAER,GAFQ,CAAX;IAGD,CALQ,CAAT;;IAMA,SAASF,YAAT,CAAsBG,CAAtB,EAAyB;MACvB,IAAIN,UAAS,GAAIM,CAAC,CAACC,MAAF,CAASC,eAAT,CAAyBC,SAA1C,EAAqD;QACnDC,OAAO,CAACC,GAAR,CAAa