 setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleWhell(event) {\n      event.preventDefault();\n\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n\n      lastY = nowY;\n    }\n\n    function handlePage(num) {\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n\n        case 2:\n          visibleSkills.value = true;\n          handleSkills();\n          break;\n      }\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    async function handleSkills() {\n      if (skillsShow.value.length === 0) {\n        for (let i = 0; i < skills.length; i++) {\n          skillsShow.value.push(skills[i]);\n          await new Promise(r => setTimeout(r, 200));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills,\n      skillsShow,\n      handlePage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CAAC,KAAD,CAA9B;IACA,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAD,CAAzB;IACA,MAAMU,MAAK,GAAI,CACb;MAAEC,GAAG,EAAEC,OAAO,CAAC,2BAAD,CAAd;MAA6CC,IAAI,EAAE;IAAnD,CADa,EAEb;MAAEF,GAAG,EAAEC,OAAO,CAAC,yBAAD,CAAd;MAA2CC,IAAI,EAAE;IAAjD,CAFa,EAGb;MAAEF,GAAG,EAAEC,OAAO,CAAC,kCAAD,CAAd;MAAoDC,IAAI,EAAE;IAA1D,CAHa,EAIb;MAAEF,GAAG,EAAEC,OAAO,CAAC,6BAAD,CAAd;MAA+CC,IAAI,EAAE;IAArD,CAJa,EAKb;MAAEF,GAAG,EAAEC,OAAO,CAAC,+BAAD,CAAd;MAAiDC,IAAI,EAAE;IAAvD,CALa,EAMb;MAAEF,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE;IAAhD,CANa,EAOb;MAAEF,GAAG,EAAEC,OAAO,CAAC,2BAAD,CAAd;MAA6CC,IAAI,EAAE;IAAnD,CAPa,EAQb;MAAEF,GAAG,EAAEC,OAAO,CAAC,yBAAD,CAAd;MAA2CC,IAAI,EAAE;IAAjD,CARa,EASb;MAAEF,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE;IAAhD,CATa,EAUb;MAAEF,GAAG,EAAEC,OAAO,CAAC,yBAAD,CAAd;MAA2CC,IAAI,EAAE;IAAjD,CAVa,EAWb;MAAEF,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE;IAAhD,CAXa,EAYb;MAAEF,GAAG,EAAEC,OAAO,CAAC,4BAAD,CAAd;MAA8CC,IAAI,EAAE;IAApD,CAZa,EAab;MAAEF,GAAG,EAAEC,OAAO,CAAC,sBAAD,CAAd;MAAwCC,IAAI,EAAE;IAA9C,CAba,EAcb;MAAEF,GAAG,EAAEC,OAAO,CAAC,sBAAD,CAAd;MAAwCC,IAAI,EAAE;IAA9C,CAda,CAAf;IAgBA,MAAMC,UAAS,GAAId,GAAG,CAAC,EAAD,CAAtB;IACA,IAAIe,KAAI,GAAI,IAAZ;IACA,IAAIC,KAAI,GAAI,IAAZ;IACA,IAAIC,IAAG,GAAI,CAAX;IACA,IAAIC,GAAE,GAAI,CAAV;IACAjB,SAAS,CAAC,MAAM;MACdkB,QAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmCC,WAAnC,EAAgD;QAAEC,OAAO,EAAE;MAAX,CAAhD;MACAH,QAAQ,CAACC,gBAAT,CAA0B,WAA1B,EAAuCG,WAAvC,EAAoD;QAAED,OAAO,EAAE;MAAX,CAApD;MACAE,WAAW,CAAC,MAAM;QAChBjB,MAAM,CAACkB,KAAP,GAAgBlB,MAAM,CAACkB,KAAP,GAAe,EAA/B,GAAsClB,MAAM,CAACkB,KAAP,GAAe,GAArD;MACD,CAFU,EAER,GAFQ,CAAX;IAGD,CANQ,CAAT;;IAOA,SAASJ,WAAT,CAAqBK,KAArB,EAA4B;MAC1BA,KAAK,CAACC,cAAN;;MACA,IAAIX,KAAJ,EAAW;QACTA,KAA