{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    let lastScrollTop = 0;\n    let rep = 0;\n    let scrennNum = 0;\n    onMounted(() => {\n      document.addEventListener(\"scroll\", handleScroll, {\n        passive: false\n      });\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleScroll() {\n      var st = window.pageYOffset || document.documentElement.scrollTop;\n      console.log(\"down\");\n      rep++;\n\n      if (st > lastScrollTop) {\n        if (rep > 12 && scrennNum < 3) {\n          scrennNum++;\n          visibleDescription.value = false;\n          console.log(\"down\");\n          rep = 0;\n        }\n      } else {\n        if (rep > 12 && scrennNum > 0) {\n          scrennNum--;\n          visibleDescription.value = true;\n          console.log(\"up\");\n          rep = 0;\n        }\n      }\n\n      lastScrollTop = st <= 0 ? 0 : st;\n      if (lastScrollTop > 0) handleWrite();\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6EA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,E