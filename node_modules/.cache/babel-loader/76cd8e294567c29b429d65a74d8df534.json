sor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleWhell(event) {\n      event.preventDefault();\n\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n\n      lastY = nowY;\n    }\n\n    function handlePage(num) {\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n\n        case 2:\n          visibleSkills.value = true;\n          break;\n      }\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for await (let char of charsText) {\n          textShow.value += char;\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills,\n      skills,\n      handlePage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4CA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CA