con name=\"chevron-double-up\" size=\"xl\" @click=\"handlePage(0)\" />\n      <div class=\"bg-dark\">\n        <div>\n          <div></div>\n          <div></div>\n          <div></div>\n          <p class=\"dark text-center\">Terminal</p>\n        </div>\n        <h4>~$ {{ textShow }}{{ cursor }}</h4>\n      </div>\n      <m-icon name=\"chevron-double-down\" size=\"xl\" @click=\"handlePage(2)\" />\n    </div>\n    <div\n      id=\"skills\"\n      class=\"bg-green white text-shadow mt-2\"\n      :class=\"{ show: visibleSkills }\"\n    >\n      <div class=\"pa-2 mx-auto\">\n        <h4>Mís habilidades</h4>\n        <br />\n        <div>\n          <div\n            v-for=\"skill in skills\"\n            :key=\"skill.name\"\n            class=\"mx-1 skill up-hover\"\n          >\n            <img :src=\"skill.img\" :alt=\"skill.name\" />\n            <p>{{ skill.name }}</p>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text =\n      '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    const skills = [\n      { img: require(\"@/assets/img/spring-3.svg\"), name: \"Springboot\" },\n      { img: require(\"@/assets/img/java-4.svg\"), name: \"Java\" },\n      { img: require(\"@/assets/img/logo-javascript.svg\"), name: \"JavaScript\" },\n      { img: require(\"@/assets/img/typescript.svg\"), name: \"Typescript\" },\n      { img: require(\"@/assets/img/angular-icon.svg\"), name: \"Angular\" },\n      { img: require(\"@/assets/img/vue-9.svg\"), name: \"Vue\" },\n      { img: require(\"@/assets/img/nodejs-2.svg\"), name: \"NodeJS\" },\n      { img: require(\"@/assets/img/html-1.svg\"), name: \"Html5\" },\n      { img: require(\"@/assets/img/css-3.svg\"), name: \"Css3\" },\n      { img: require(\"@/assets/img/sass-1.svg\"), name: \"Sass\" },\n      { img: require(\"@/assets/img/php-1.svg\"), name: \"Php\" },\n      { img: require(\"@/assets/img/laravel-2.svg\"), name: \"Laravel\" },\n      { img: require(\"@/assets/img/tux.svg\"), name: \"Linux\" },\n      { img: require(\"@/assets/img/git.svg\"), name: \"Git\" },\n    ];\n    let lastY = null;\n    let timer = true;\n    let page = 0;\n    let rep = 0;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleWhell, { passive: false });\n      document.addEventListener(\"touchmove\", handleTouch, { passive: false });\n      setInterval(() => {\n        cursor.value ? (cursor.value = \"\") : (cursor.value = \"|\");\n      }, 300);\n    });\n    function handleWhell(event) {\n      event.preventDefault();\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n      lastY = nowY;\n    }\n    function handlePage(num) {\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n        case 2:\n          visibleSkills.value = true;\n          break;\n      }\n    }\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for await (let char of charsText) {\n          textShow.value += char;\n          await new Promise((r) => setTimeout(r, 50));\n        }\n      }\n    }\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills,\n      skills,\n      handlePage,\n    };\n  },\n};\n</script>\n\n<style lang=\"scss\">\nbody {\n  overflow: hidden;\n}\nheader {\n  z-index: 0;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  background-image: url(\"@/assets/img/header-background.jpg\");\n  background-position: center;\n  background-size: cover;\n  height: 100vh;\n  width: 100%;\n  text-align: center;\n  h3 {\n    padding-bottom: 2em;\n  }\n  img {\n    margin: auto;\n    max-width: 20%;\n  }\n}\n#description {\n  background: rgb(63, 94, 251);\n  background: linear-gradient(\n    90deg,\n    rgba(63, 94, 251, 1) 0%,\n    rgba(252, 70, 226, 1) 100%\n  );\n  color: rgb(35, 228, 35);\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 1;\n  margin-top: 100%;\n  opacity: 0;\n  position: absolute;\n  transition: all 0.5s ease-in-out;\n  > div {\n    overflow: hidden;\n    border-radius: 5px;\n    border: 4px solid rgb(223, 211, 211);\n    padding: 5%;\n    margin: 5%;\n    min-height: 60vh;\n    position: relative;\n    > div {\n      position: absolute;\n      top: 0;\n      left: 0;\n      right: 0;\n      width: 100%;\n      background-color: rgb(223, 211, 211);\n      height: 25px;\n      display: grid;\n      gap: 10px;\n      grid-template-columns: repeat(auto-fit, 15px);\n      align-items: center;\n      justify-content: left;\n      padding-left: 20px;\n      div {\n        height: 15px;\n        width: 15x;\n        border-radius: 100%;\n      }\n      :nth-child(1) {\n        background-color: red;\n      }\n      :nth-child(2) {\n        background-color: yellow;\n      }\n      :nth-child(3) {\n        background-color: rgb(31, 197, 31);\n      }\n      :nth-child(4){\n        grid-row-end: span;\n        border: 1px solid red;\n      }\n    }\n  }\n}\n#skills {\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 2;\n  margin-top: 100%;\n  margin-left: 100%;\n  width: 100vw;\n  height: 100vh;\n  position: absolute;\n  div {\n    div {\n      display: grid;\n      gap: 1em;\n      grid-template-columns: repeat(auto-fit, minmax(8em, 1fr));\n      align-items: center;\n    }\n  }\n  transition: all 0.5s ease-in-out;\n}\n.skill {\n  display: inline-block;\n  text-align: center;\n  font-weight: 600;\n  img {\n    margin: auto;\n    max-width: 100px;\n    max-height: 100px;\n  }\n}\n.up-hover {\n  transition: all 0.3s;\n  &:hover {\n    transform: translateY(-0.5em);\n  }\n}\n.show {\n  margin-top: 0 !important;\n  margin-left: 0 !important;\n  opacity: 1 !important;\n}\n.mdi-chevron-double-down {\n  animation: chevron-down 2s infinite;\n  position: absolute;\n  bottom: 0.1em;\n  left: 0;\n  right: 0;\n  margin: auto;\n  text-align: center;\n  color: white;\n}\n@keyframes chevron-down {\n  0% {\n    transform: translateY(0);\n  }\n  80% {\n    transform: translateY(0);\n  }\n  90% {\n    transform: translateY(0.2em);\n  }\n  100% {\n    transform: translateY(0);\n  }\n}\n.mdi-chevron-double-up {\n  animation: chevron-up 2s infinite;\n  position: absolute;\n  top: 0.1em;\n  left: 0;\n  right: 0;\n  margin: auto;\n  text-align: center;\n  color: white;\n}\n@keyframes chevron-up {\n  0% {\n    transform: translateY(0);\n  }\n  80% {\n    transform: translateY(0);\n  }\n  90% {\n    transform: translateY(-0.2em);\n  }\n  100% {\n    transform: translateY(0);\n  }\n}\n@media (max-width: 900px) {\n  header {\n    font-size: 0.7em;\n    h3 {\n      padding: 0 5% 4em 5%;\n    }\n    img {\n      max-width: 40%;\n    }\n  }\n  #description {\n    font-size: 0.7em;\n  }\n  .skill {\n    img {\n      max-width: 50px;\n      max-height: 50px;\n    }\n  }\n  #skills {\n    padding: 5%;\n    div {\n      div {\n        gap: 0.2em;\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}