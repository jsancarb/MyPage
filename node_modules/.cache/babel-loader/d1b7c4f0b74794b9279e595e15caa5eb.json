{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    const skills = [{\n      img: \"@/assets/img/java-4.svg\",\n      name: \"Java\"\n    }, {\n      img: \"@/assets/img/spring-3.svg\",\n      name: \"Springboot\"\n    }, {\n      img: \"@/assets/img/logo-javascript.svg\",\n      name: \"JavaScript\"\n    }, {\n      img: \"@/assets/img/typescript.svg\",\n      name: \"Typescript\"\n    }, {\n      img: \"@/assets/img/angular-icon.svg\",\n      name: \"Angular\"\n    }, {\n      img: \"@/assets/img/vue-9.svg\",\n      name: \"Vue\"\n    }, {\n      img: \"@/assets/img/nodejs-2.svg\",\n      name: \"NodeJS\"\n    }, {\n      img: \"@/assets/img/html-1.svg\",\n      name: \"Html5\"\n    }, {\n      img: \"@/assets/img/css-3.svg\",\n      name: \"Css3\"\n    }, {\n      img: \"@/assets/img/sass-1.svg\",\n      name: \"Sass\"\n    }, {\n      img: \"@/assets/img/php-1.svg\",\n      name: \"Php\"\n    }, {\n      img: \"@/assets/img/laravel-2.svg\",\n      name: \"Laravel\"\n    }, {\n      img: \"@/assets/img/tux.svg\",\n      name: \"Linux\"\n    }, {\n      img: \"@/assets/img/git.svg\",\n      name: \"Git\"\n    }];\n    let lastY = null;\n    let timer = true;\n    let page = 0;\n    let rep = 0;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleWhell, {\n        passive: false\n      });\n      document.addEventListener(\"touchmove\", handleTouch, {\n        passive: false\n      });\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleWhell(event) {\n      event.preventDefault();\n\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n\n      lastY = nowY;\n    }\n\n    function handlePage(num) {\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n\n        case 2:\n          visibleSkills.value = true;\n          break;\n      }\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills,\n      skills\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuCA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CAAC,KAAD,CAA9B;IACA,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAD,CAAzB;IACA,MAAMU,MAAK,GAAI,CACb;MAAEC,GAAG,EAAE,yBAAP;MAAkCC,IAAI,EAAE;IAAxC,