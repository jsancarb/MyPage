{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    let lastScrollTop = 0;\n    let timer = true;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleScroll, {\n        passive: false\n      });\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleScroll(event) {\n      event.preventDefault();\n\n      if (event.wheelDeltaY < 0 && timer) {\n        timer = false;\n        console.log(\"up\");\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n      } else if (event.wheelDeltaY > 0 && timer) {\n        timer = false;\n        console.log(\"down\");\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n      }\n\n      if (lastScrollTop > 0) handleWrite();\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiFA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CAAC,KAAD,CAA9B;IACA,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAD,CAAzB;IACA,IAAIU,aAAY,GAAI,CAApB;IACA,IAAIC,KAAI,GAAI,IAAZ;IACAV,SAAS,CAAC,MAAM;MACdW,QAAQ,CAACC,gBAAT,CAA0B,OAA1B,EAAmCC,YAAnC,EAAiD;QAAEC,OAAO,EAAE;MAAX,CAAjD;MACAC,WAAW,CAAC,MAAM;QAChBT,MAAM,CAACU,KAAP,GAAgBV,MAAM,CAACU,KAAP,GAAe,EAA/B,GAAsCV,MAAM,CAACU,KAAP,GAAe,GAArD;MACD,CAFU,EAER,GAFQ,CAAX;IAGD,CALQ,CAAT;;IAMA,SAASH,YAAT,CAAsBI,KAAtB,EAA6B;MAC3BA,KAAK,CAACC,cAAN;;MACA,IAAID,KAAK,CAACE,WAAN,GAAoB,CAApB,IAAyBT,KAA7B,EAAoC;QAClCA,KAAI,GAAI,KAAR;QACAU,OAAO,CAACC,GAAR,CAAY,IAAZ;QACAC,UAAU,CAAC,MAAM;UACfZ,KAAI,GAAI,IAAR;QACD,CAFS,EAEP,GAFO,CAAV;MAGF,CANA,MAMO,IAAIO,KAAK,CAACE,WAAN,GAAoB,CAApB,IAAyBT,KAA7B,EAAoC;QACzCA,KAAI,GAAI,KAAR;QACAU,OAAO,CAACC,GAAR,CAAY,MAAZ;QACAC,UAAU,CAAC,MAAM;UACfZ,KAAI,GAAI,IAAR;QACD,CAFS,EAEP,GAFO,CAAV;MAGF;;MAEA,IAAID,aAAY,GAAI,CAApB,EAAuBc,WAAW;IACpC;;IACA,eAAeA,WAAf,GAA6B;MAC3B,IAAIlB,QAAQ,CAACW,KAAT,CAAeQ,MAAf,KAA0B,CAA9B,EAAiC;QAC/B,KAAK,IAAIC,IAAI,CAAb,EAAgBA,IAAItB,SAAS,CAACqB,MAA9B,EAAsCC,CAAC,EAAvC,EAA2C;UACzCpB,QAAQ,CAACW,KAAT,IAAkBb,SAAS,CAACsB,CAAD,CAA3B;UACA,MAAM,IAAIC,OAAJ,CAAaC,CAAD,IAAOL,UAAU,CAACK,CAAD,EAAI,EAAJ,CAA7B,CAAN;QACF;MACF;IACF;;IACA,OAAO;MAAEtB,QAAF;MAAYC,MAAZ;MAAoBC,kBAApB;MAAwCC;IAAxC,CAAP;EACD;;AA5CY,CAAf","names":["ref","onMounted","setup","text","charsText","split","textShow","cursor","visibleDescription","visibleSkills","lastScrollTop","timer","document","addEventListener","handleScroll","passive","setInterval","value","event","preventDefault","wheelDeltaY","console","log","setTimeout","handleWrite","length","i","Promise","r"],"sourceRoot":"","sources":["/home/jsancarb/Proyectos/my-portfolio/src/pages/IndexPage.vue"],"sourcesContent":["<template>\n  <header id=\"header\" class=\"white text-shadow\">\n    <img src=\"@/assets/img/yo.svg\" alt=\"logo\" class=\"pa-2\" />\n    <div>\n      <h1>¡Hola! me llamo Jesús y soy desarrollador Fullstack</h1>\n    </div>\n  </header>\n  <div\n    id=\"description\"\n    class=\"bg-dark white\"\n    :class=\"{ show: visibleDescription }\"\n  >\n    <h4>~$ {{ textShow }}{{ cursor }}</h4>\n  </div>\n  <div id=\"skills\" class=\"bg-green white text-shadow mt-2\">\n    <div class=\"pa-2 mx-auto\">\n      <h4>Mís habilidades</h4>\n      <br />\n      <div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/java-4.svg\" alt=\"Java\" />\n          <p>Java</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/spring-3.svg\" alt=\"Springboot\" />\n          <p>Spring Boot</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/logo-javascript.svg\" alt=\"JavaScript\" />\n          <p>JavaScript</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/typescript.svg\" alt=\"Typescript\" />\n          <p>TypeScript</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/angular-icon.svg\" alt=\"Angular\" />\n          <p>Angular</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/vue-9.svg\" alt=\"Vue\" />\n          <p>Vue</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/nodejs-2.svg\" alt=\"NodeJS\" />\n          <p>NodeJS</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/html-1.svg\" alt=\"Html5\" />\n          <p>HTML 5</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/css-3.svg\" alt=\"Css3\" />\n          <p>CSS 3</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/sass-1.svg\" alt=\"Sass\" />\n          <p>Sass</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/php-1.svg\" alt=\"Php\" />\n          <p>PHP</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/laravel-2.svg\" alt=\"Laravel\" />\n          <p>Laravel</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/tux.svg\" alt=\"Linux\" />\n          <p>Linux</p>\n        </div>\n        <div class=\"mx-1 skill up-hover\">\n          <img src=\"@/assets/img/git.svg\" alt=\"Git\" />\n          <p>Git</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text =\n      '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    const visibleDescription = ref(false);\n    const visibleSkills = ref(false);\n    let lastScrollTop = 0;\n    let timer = true;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleScroll, { passive: false });\n      setInterval(() => {\n        cursor.value ? (cursor.value = \"\") : (cursor.value = \"|\");\n      }, 300);\n    });\n    function handleScroll(event) {\n      event.preventDefault();\n      if (event.wheelDeltaY < 0 && timer) {\n        timer = false;\n        console.log(\"up\");\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n      } else if (event.wheelDeltaY > 0 && timer) {\n        timer = false;\n        console.log(\"down\");\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n      }\n\n      if (lastScrollTop > 0) handleWrite();\n    }\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise((r) => setTimeout(r, 50));\n        }\n      }\n    }\n    return { textShow, cursor, visibleDescription, visibleSkills };\n  },\n};\n</script>\n\n<style lang=\"scss\">\nheader {\n  display: flex;\n  justify-content: center;\n  align-content: center;\n  background-image: url(\"@/assets/img/background.svg\");\n  background-color: black;\n  background-position: center;\n  background-size: cover;\n  height: 100vh;\n  width: 100%;\n  font-size: 0.5vw;\n  img {\n    margin: auto;\n    max-width: 10vw;\n  }\n}\n#description {\n  display: none;\n  overflow: hidden;\n  top: 0;\n  left: 0;\n  z-index: 200;\n  position: absolute;\n  padding: 2em 4em 2em 4em;\n  font-size: 0.8vw;\n  height: 100vh;\n  width: 100vw;\n}\n#skills {\n  display: none;\n  height: 100vh;\n  div {\n    div {\n      display: grid;\n      gap: 1em;\n      grid-template-columns: repeat(auto-fit, minmax(8em, 1fr));\n      align-items: center;\n    }\n  }\n}\n.skill {\n  display: inline-block;\n  text-align: center;\n  font-weight: 600;\n  img {\n    margin: auto;\n    max-width: 100px;\n    max-height: 100px;\n  }\n}\n.up-hover {\n  transition: all 0.3s;\n  &:hover {\n    transform: translateY(-0.5em);\n  }\n}\n.show {\n  display: block !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}