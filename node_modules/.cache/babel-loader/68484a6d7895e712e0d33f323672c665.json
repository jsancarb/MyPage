astY = null;\n    let timer = true;\n    let page = 0;\n    let rep = 0;\n    let writing = false;\n    onMounted(() => {\n      document.addEventListener(\"wheel\", handleWhell, {\n        passive: false\n      });\n      document.addEventListener(\"touchmove\", handleTouch, {\n        passive: false\n      });\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleWhell(event) {\n      event.preventDefault();\n\n      if (timer) {\n        timer = false;\n        setTimeout(() => {\n          timer = true;\n        }, 300);\n\n        if (event.wheelDeltaY < 0 && page < 2) {\n          page++;\n          handlePage(page);\n        } else if (event.wheelDeltaY > 0 && page > 0) {\n          page--;\n          handlePage(page);\n        }\n      }\n    }\n\n    function handleTouch(event) {\n      event.preventDefault();\n      rep++;\n      let nowY = event?.touches[0].clientY;\n\n      if (nowY < lastY && page < 2 && rep > 10) {\n        page++;\n        handlePage(page);\n        rep = 0;\n      } else if (nowY > lastY && page > 0 && rep > 10) {\n        page--;\n        handlePage(page);\n        rep = 0;\n      }\n\n      lastY = nowY;\n    }\n\n    function handlePage(num) {\n      console.log(num);\n\n      switch (num) {\n        case 0:\n          visibleDescription.value = false;\n          visibleSkills.value = false;\n          break;\n\n        case 1:\n          visibleDescription.value = true;\n          visibleSkills.value = false;\n          handleWrite();\n          break;\n\n        case 2:\n          visibleSkills.value = true;\n          handleSkills();\n          break;\n      }\n    }\n\n    async function handleWrite() {\n      if (!writing) {\n        textShow.value = \"\";\n        writing = true;\n\n        for await (let char of charsText) {\n          textShow.value += char;\n          await new Promise(r => setTimeout(r, 50));\n        }\n\n        writing = false;\n      }\n    }\n\n    async function handleSkills() {\n      for (let skill of skills.value) {\n        skill.visible = false;\n      }\n\n      await new Promise(r => setTimeout(r, 500));\n\n      for await (let skill of skills.value) {\n        skill.visible = true;\n        await new Promise(r => setTimeout(r, 100));\n      }\n    }\n\n    return {\n      textShow,\n      cursor,\n      visibleDescription,\n      visibleSkills,\n      skills,\n      handlePage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA0CA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,MAAMQ,kBAAiB,GAAIR,GAAG,CAAC,KAAD,CAA9B;IACA,MAAMS,aAAY,GAAIT,GAAG,CAAC,KAAD,CAAzB;IACA,MAAMU,MAAK,GAAIV,GAAG,CAAC,CACjB;MACEW,GAAG,EAAEC,OAAO,CAAC,2BAAD,CADd;MAEEC,IAAI,EAAE,YAFR;MAGEC,OAAO,EAAE;IAHX,CADiB,EAMjB;MAAEH,GAAG,EAAEC,OAAO,CAAC,yBAAD,CAAd;MAA2CC,IAAI,EAAE,MAAjD;MAAyDC,OAAO,EAAE;IAAlE,CANiB,EAOjB;MACEH,GAAG,EAAEC,OAAO,CAAC,kCAAD,CADd;MAEEC,IAAI,EAAE,YAFR;MAGEC,OAAO,EAAE;IAHX,CAPiB,EAYjB;MACEH,GAAG,EAAEC,OAAO,CAAC,6BAAD,CADd;MAEEC,IAAI,EAAE,YAFR;MAGEC,OAAO,EAAE;IAHX,CAZiB,EAiBjB;MACEH,GAAG,EAAEC,OAAO,CAAC,+BAAD,CADd;MAEEC,IAAI,EAAE,SAFR;MAGEC,OAAO,EAAE;IAHX,CAjBiB,EAsBjB;MAAEH,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE,KAAhD;MAAuDC,OAAO,EAAE;IAAhE,CAtBiB,EAuBjB;MACEH,GAAG,EAAEC,OAAO,CAAC,2BAAD,CADd;MAEEC,IAAI,EAAE,QAFR;MAGEC,OAAO,EAAE;IAHX,CAvBiB,EA4BjB;MACEH,GAAG,EAAEC,OAAO,CAAC,yBAAD,CADd;MAEEC,IAAI,EAAE,OAFR;MAGEC,OAAO,EAAE;IAHX,CA5BiB,EAiCjB;MAAEH,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE,MAAhD;MAAwDC,OAAO,EAAE;IAAjE,CAjCiB,EAkCjB;MAAEH,GAAG,EAAEC,OAAO,CAAC,yBAAD,CAAd;MAA2CC,IAAI,EAAE,MAAjD;MAAyDC,OAAO,EAAE;IAAlE,CAlCiB,EAmCjB;MAAEH,GAAG,EAAEC,OAAO,CAAC,wBAAD,CAAd;MAA0CC,IAAI,EAAE,KAAhD;MAAuDC,OAAO,EAAE;IAAhE,CAnCiB,EAoCjB;MACEH,GAAG,EAAEC,OAAO,CAAC,4BAAD,CADd;MAEEC,IAAI,EAAE,SAFR;M