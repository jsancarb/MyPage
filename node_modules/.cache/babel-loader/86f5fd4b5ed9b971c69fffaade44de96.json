{"ast":null,"code":"import { ref, onMounted } from \"vue\";\nexport default {\n  setup() {\n    const text = '\"La tecnología me ha motivado siempre a evolucionar y poder reinventarme, el desarrollo web puedo decir que es mi pasión y ahora también mi oficio\"';\n    const charsText = text.split(\"\");\n    const textShow = ref(\"\");\n    const cursor = ref(\"|\");\n    let lastScrollTop = 0;\n    let rep = 0;\n    onMounted(() => {\n      document.addEventListener(\"scroll\", handleScroll, false);\n      setInterval(() => {\n        cursor.value ? cursor.value = \"\" : cursor.value = \"|\";\n      }, 300);\n    });\n\n    function handleScroll() {\n      var st = window.pageYOffset || document.documentElement.scrollTop;\n\n      if (st > lastScrollTop) {\n        rep++;\n\n        if (rep > 20) {\n          console.log(\"scrollDown\");\n          rep = 0;\n        }\n      } else {\n        rep++;\n\n        if (rep > 20) {\n          console.log(\"scrollUp\");\n          rep = 0;\n        }\n      }\n\n      lastScrollTop = st <= 0 ? 0 : st;\n      if (lastScrollTop > 0) handleWrite();\n    }\n\n    async function handleWrite() {\n      if (textShow.value.length === 0) {\n        for (let i = 0; i < charsText.length; i++) {\n          textShow.value += charsText[i];\n          await new Promise(r => setTimeout(r, 50));\n        }\n      }\n    }\n\n    return {\n      textShow,\n      cursor\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6EA,SAASA,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AACA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,IAAG,GACP,qJADF;IAEA,MAAMC,SAAQ,GAAID,IAAI,CAACE,KAAL,CAAW,EAAX,CAAlB;IACA,MAAMC,QAAO,GAAIN,GAAG,CAAC,EAAD,CAApB;IACA,MAAMO,MAAK,GAAIP,GAAG,CAAC,GAAD,CAAlB;IACA,IAAIQ,aAAY,GAAI,CAApB;IACA,IAAIC,GAAE,GAAI,CAAV;IACAR,SAAS,CAAC,MAAM;MACdS,QAAQ,CAACC,gBAAT,CAA0B,QAA1B,EAAoCC,YAApC,EAAkD,KAAlD;MACAC,WAAW,CAAC,MAAM;QAChBN,MAAM,CAACO,KAAP,GAAgBP,MAAM,CAACO,KAAP,GAAe,EAA/B,GAAsCP,MAAM,CAACO,KAAP,GAAe,GAArD;MACD,CAFU,EAER,GAFQ,CAAX;IAGD,CALQ,CAAT;;IAMA,SAASF,Y